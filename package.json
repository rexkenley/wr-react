{
  "name": "wr-react",
  "version": "1.0.0",
  "description": "A starter kit for d365 om premise",
  "scripts": {
    "storybook": "start-storybook",
    "lint": "eslint --ext .jsx,.js src/",
    "test": "npx jest --verbose ./test",
    "test:update": "npx jest --updateSnapshot --verbose ./test",
    "test:output": "npx jest --verbose ./test --json --outputFile=./test/__results__/.jest-test-results.json",
    "doc": "jsdoc -c jsdoc.json",
    "//Chunk": "****************** -- source.js --o destination.js",
    "build:chunk:dev": "%npm_package_scripts_buildDevChunk%",
    "build:chunk:prd": "%npm_package_scripts_buildPrdChunk%",
    "//NPM": "*****************************************************************",
    "buildDev": "npx lint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npx lint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js"
  },
  "author": "Rex Kenley Tan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.4",
    "@types/jest": "^25.1.3",
    "@uifabric/icons": "^7.3.12",
    "core-js": "^3.6.4",
    "office-ui-fabric-react": "^7.96.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-jest": "^5.3.13",
    "@storybook/addon-storyshots": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.3",
    "prettier": "1.19.1",
    "pug-cli": "^1.0.0-alpha6",
    "react-test-renderer": "^16.12.0"
  }
}
